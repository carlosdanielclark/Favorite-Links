<div class="row">
  <div class="col s12 center_conten">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <div class="section">
            <h2>SingUp</h2>
        </div>
        <div class="section">
            <form action="/singup"  method="post">
                <div class="input-field section">
                    <input type="text" name="fullname" id="fullname" class="validate white-text" autofocus>
                    <label for="fullname">Full Name</label>
                </div>

                <div class="input-field section">
                    <input type="text" name="username" id="username" class="validate white-text">
                    <label for="username">User Name</label>
                </div>

                <div class="input-field section">
                    <input type="password" name="password" id="password" class="validate white-text">
                    <label for="password">Password</label>
                </div>

                <div class="section">
                    <button class="btn btn_form blue-grey lighten-2 col s12">SingUp
                    </button>
                </div>
            </form>
        </div>
      </div>
    </div>
  </div>
</div>

